@model List<OmxTechNet.Models.ViewModel.AboutusViewModel>
<!--
    about-us start
    ============================== -->
<section id="about-us" style="background-color:#282828;">
    <div class="container">
        <div class="row">
            <div class="col-md-4">
                <div class="block">
                    @foreach (var itm in Model)
                    {
                        <img class="wow fadeInUp" data-wow-duration="300ms" data-wow-delay="400ms" src="@Url.Content(@itm.a_img)" alt="cooker-img" style="width:20rem;">
                        <h1 class="heading wow fadeInUp" data-wow-duration="400ms" data-wow-delay="500ms">
                            <span style="color:#EF038E;">@itm.a_title</span>
                        </h1>
                        <p class="wow fadeInUp" data-wow-duration="300ms" data-wow-delay="600ms">
                          @itm.a_desc
                    </p >
                    }

</div>
            </div><!-- .col-md-12 close -->
        <div class="col-md-8">
            <img src="~/Content/images/products/fullD.jpg" class="img-responsive" />
        </div>
        </div><!-- .row close -->
    </div><!-- .containe close -->
</section><!-- #call-to-action close -->
